<action>
	<icon>utilities-terminal-symbolic</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>2-2</unique-id>
	<command>exo-open --working-directory %d --launch TerminalEmulator</command>
	<description>Open terminal in specified directory</description>
	<range></range>
	<patterns>*</patterns>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
